{
  "version": "0.1.0",
  "name": "community-fridge-map",
  "description": "A mobile friendly website that displays an interactive map of all the community fridges in and around New York City.",
  "keywords": [
    "nyc",
    "food bank",
    "community fridge",
    "map"
  ],
  "imports": {
    "#schema/*": "./src/schema/*"
  },
  "homepage": "https://github.com/CollectiveFocus/CFM_Frontend#readme",
  "author": "Collective Focus <info@collectivefocus.site> (https://collectivefocus.site/)",
  "contributors": [
    "Andrew R",
    "Bernard Martis (https://bernardm.github.io/)",
    "Hamaad Chughtai (https://github.com/Hamaad102)",
    "Ioana Tiplea (https://github.com/ioanat94)",
    "Trillium Smith (https://github.com/Spiteless)",
    "Weston Norwood (https://github.com/wheninseattle)"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CollectiveFocus/CFM_Frontend.git"
  },
  "bugs": {
    "url": "https://github.com/CollectiveFocus/CFM_Frontend/issues"
  },
  "directories": {
    "doc": "./docs"
  },
  "scripts": {
    "preinstall": "npm i -g prettier svgo only-allow lint-staged concurrently json-server && only-allow yarn",
    "postinstall": "npx simple-git-hooks",
    "dev": "concurrently --names \"MOCK,NEXT\" --prefix-colors \"yellow,green\" --kill-others \"json-server --watch mock/fake-data.json --routes mock/server-routes.json --static mock/img -H 127.0.0.1\" \"npm exec --offline -- next -p 4000\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "style": "prettier --write --ignore-unknown . && svgo -qrf public/"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "packageManager": "yarn@1.22.15",
  "simple-git-hooks": {
    "pre-commit": "lint-staged --quiet --no-stash --concurrent true",
    "commit-msg": "node ci/commit-msg.mjs $1"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.1",
    "formik": "^2.2.9",
    "leaflet": "^1.8.0",
    "next": "12.2.2",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-leaflet": "^4.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "picocolors": "^1.0.0",
    "simple-git-hooks": "^2.7.0"
  }
}
